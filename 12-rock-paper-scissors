public Integer rockPaperScissors(String player1, String player2) {
    //code here
    
   Map<String,Integer> resultComp = new Map<String,Integer> ();
   resultComp.put('rock','scissors');
   resultComp.put('scissors','paper');
   resultComp.put('paper','rock');
   if(player1 == player2) return 0;
   else if(resultComp.get(player1) == player2) return 1;
   else return 2;

}