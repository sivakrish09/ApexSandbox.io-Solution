public Boolean isPrime(Integer num) {
    //code here
    Boolean result = false;
    if(num == 2)
    {
        result = true;
    }
    else{
            for(Integer i = 2 ; i < num; i++)
            {
                if(Math.mod(num,i) == 0)
                {
                    result = false;
                    break;
                }
                else
                {
                    result = true;
                }
            }
    }

    return result;

}