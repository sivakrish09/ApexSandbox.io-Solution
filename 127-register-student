public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c student = new apxio__Student__c();
    student.Name = name;
    student.apxio__Email__c = email;
    student.apxio__Phone__c = phone;

    // Insert the student record
    Insert student;

    // Query for the inserted student using the Id
    apxio__Student__c students = [SELECT Id, apxio__Registration_Number__c FROM apxio__Student__c WHERE Id = :student.Id];

    // Access the Registration Number and debug
    String result = students.apxio__Registration_Number__c;
    return result;
} 